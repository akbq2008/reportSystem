<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">


	<servlet>
		<servlet-name>HandleLogin</servlet-name>
		<servlet-class>com.servlet.HandleLogin</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>HandleLogin</servlet-name>
		<url-pattern>/HandleLogin</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>HandleUpdate</servlet-name>
		<servlet-class>com.servlet.HandleUpdate</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>HandleUpdate</servlet-name>
		<url-pattern>/HandleUpdate</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>HandleRegister</servlet-name>
		<servlet-class>com.servlet.HandleRegister</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>HandleRegister</servlet-name>
		<url-pattern>/HandleRegister</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>selectContent</servlet-name>
		<servlet-class>com.content.servlet.selectContent</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>selectContent</servlet-name>
		<url-pattern>/selectContent</url-pattern>
	</servlet-mapping>


</web-app>
